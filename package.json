    "rich",
    "editor",
    "html",
    "riotjs",
    "component"
  ],
  "peerDependencies": {
    "riot": "^2.0.14"
  },
  "author": "Avner Peled",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ListnPlay/riot-froala/issues"
  },
  "homepage": "https://github.com/ListnPlay/riot-froala",
  "devDependencies": {
    "beefy": "^2.1.5"
  },
  "dependencies": {
    "cssify": "^0.7.0"
  },
  "browserify": { "transform": [ "cssify"]  }
}
